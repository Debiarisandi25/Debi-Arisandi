print ("hitung luas ruangan")
a=float(input("masukan panjang ruangan:"))
b=float(input("masukan lebar ruangan:"))
sendal=input('masukan satuan (meter atau inci:')

luas= a*b
print ("luas ruangan dengan panjang", a, "dan lebar", b,'adalah:',luas,sendal)
