data_debby= read.csv("C:/Users/HP/Downloads/Dataset/DirectMarketing.csv")
View(data_debby)
hist(data_debby$AmountSpent)
hist(data_debby$AmountSpent[data_debby$Married == "Single"])
hist(data_debby$AmountSpent[data_debby$Married == "Married"])
d=density(data_debby$AmountSpent)
plot(d)
d=density(data_debby$AmountSpent[data_debby$Married == "Married"])
plot(d)

